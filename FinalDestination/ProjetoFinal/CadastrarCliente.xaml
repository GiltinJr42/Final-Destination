<Window x:Class="ProjetoFinal.CadastrarCliente"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjetoFinal"
        mc:Ignorable="d"
        Title="CadastrarCliente" Height="285.648" Width="288.917">
    <Grid>
        <GroupBox x:Name="groupBox" Header="" Margin="10">
            <Grid Margin="0,0,-2,0">
                <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="18,20,0,0" TextWrapping="Wrap" Text="Usuário:" VerticalAlignment="Top"/>
                <TextBox x:Name="NomeCliente" Height="23" Margin="89,19,20,0" TextWrapping="Wrap" Text="AnaLigia" VerticalAlignment="Top" />
                <TextBlock x:Name="textBlock_Copy" HorizontalAlignment="Left" Margin="18,53,0,0" TextWrapping="Wrap" Text="Senha:" VerticalAlignment="Top"/>
                <PasswordBox x:Name="SenhaCliente" Password="Senha" Height="23" Margin="89,55,20,0" VerticalAlignment="Top" />
                <TextBlock x:Name="textBlock_Copy1" HorizontalAlignment="Left" Margin="18,89,0,0" TextWrapping="Wrap" Text="E-mail:" VerticalAlignment="Top"/>
                <TextBox x:Name="EmailCliente" Height="23" Margin="89,88,20,0" TextWrapping="Wrap" Text="@email.com" VerticalAlignment="Top" />
                <Button x:Name="button" Content="Cadastrar cliente" Margin="18,0,20,10" VerticalAlignment="Bottom" Click="Cads" d:LayoutOverrides="VerticalAlignment" Height="26"/>
     
                <Button x:Name="button1" Content="Adicionar Ícone" Margin="18,0,20,62" VerticalAlignment="Bottom" Height="23"/>
            </Grid>
        </GroupBox>

    </Grid>
</Window>
