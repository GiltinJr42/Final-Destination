<Window x:Class="ProjetoFinal.CadastrarCliente"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjetoFinal"
        mc:Ignorable="d"
        Title="CadastrarCliente" Height="332.123" Width="288.917">
    <Grid>
        <GroupBox x:Name="groupBox" Header="" Margin="10">
            <Grid Margin="0,0,-2,0">
                <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="18,20,0,0" TextWrapping="Wrap" Text="Usuário:" VerticalAlignment="Top"/>
                <TextBox x:Name="NomeCliente" Height="23" Margin="89,19,20,0" TextWrapping="Wrap" Text="AnaLigia" VerticalAlignment="Top" d:LayoutOverrides="HorizontalAlignment"/>
                <TextBlock x:Name="textBlock_Copy" HorizontalAlignment="Left" Margin="18,53,0,0" TextWrapping="Wrap" Text="Senha:" VerticalAlignment="Top"/>
                <TextBox x:Name="SenhaCliente" Height="23" Margin="89,55,20,0" TextWrapping="Wrap" Text="*****" VerticalAlignment="Top" d:LayoutOverrides="HorizontalAlignment"/>
                <TextBlock x:Name="textBlock_Copy1" HorizontalAlignment="Left" Margin="18,89,0,0" TextWrapping="Wrap" Text="E-mail:" VerticalAlignment="Top"/>
                <TextBox x:Name="EmailCliente" Height="23" Margin="89,88,20,0" TextWrapping="Wrap" Text="@email.com" VerticalAlignment="Top" d:LayoutOverrides="HorizontalAlignment"/>
                <TextBlock x:Name="textBlock_Copy3" HorizontalAlignment="Left" Margin="18,0,0,112" TextWrapping="Wrap" Text="Data de contrato:" VerticalAlignment="Bottom"/>
                <TextBox x:Name="DataCCliente" Margin="126,128,20,106" TextWrapping="Wrap" Text="01/01/2001"/>
                <Button x:Name="button" Content="Cadastrar cliente" Margin="18,0,20,19" VerticalAlignment="Bottom" Click="Cads"/>
                <TextBox x:Name="tel" Height="23" Margin="0,0,46,71" TextWrapping="Wrap" Text="" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="79"/>
                <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="18,0,0,71" TextWrapping="Wrap" Text="Telefone" VerticalAlignment="Bottom"/>
            </Grid>
        </GroupBox>

    </Grid>
</Window>
