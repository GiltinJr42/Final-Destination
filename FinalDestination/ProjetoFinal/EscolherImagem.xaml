<Window x:Class="ProjetoFinal.EscolherImagem"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjetoFinal"
        mc:Ignorable="d"
        Title="EscolherImagem" Height="428.8" Width="298">
    <Grid>

        <Grid Height="224">


            <GroupBox x:Name="groupbox" Header="Pegar Icone do computador" Margin="0,-88">
                <Grid Margin="10">
                    <Button x:Name="BtnLoadFromFile" Content="Adicionar Icone ao cliente" VerticalAlignment="Top" d:LayoutOverrides="Height" Click="BtnLoadFromFile_Click"/>
                    <Image x:Name="imgDynamic" Margin="0,49,0,75"/>
                    <Button x:Name="button" Content="Adicionar Imagem" Margin="10,0,10,10" VerticalAlignment="Bottom" d:LayoutOverrides="Height"/>

                </Grid>
            </GroupBox>


        </Grid>



    </Grid>
</Window>
